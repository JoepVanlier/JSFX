@init

function init_vowels(freemem)
(
  vowel = freemem;

  handle = file_open("partials_dependencies/saike_vowel_data.ske");
  file_mem(handle, vowel, 22050 * 8);
  file_close(handle);
  
  female_vowel = freemem + 22050 * 8;
  
  handle = file_open("partials_dependencies/saike_female_vowel_data.ske");
  file_mem(handle, female_vowel, 22050 * 8);
  file_close(handle);
  
  
  freemem + 22050 * 16;
);

