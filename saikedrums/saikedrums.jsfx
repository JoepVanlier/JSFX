@init
last_reinit_state = -1;
img_w = 256;
img_h = 150;
osc_downsample_factor = 64;
osc_skipfactor = 2;
osc_buffer = img_w * osc_downsample_factor;

CURRENT_VERSION = 3;
version = CURRENT_VERSION;

NOTE_KICK = 1;
NOTE_SNARE = 2;
NOTE_CLAP = 3;
NOTE_CLOSED_HAT = 4;
NOTE_OPEN_HAT = 5;
NOTE_RIDE = 6;
NOTE_SHAKER = 7;
NOTE_HI_TOM = 8;
NOTE_MID_TOM = 9;
NOTE_LOW_TOM = 10;
NOTE_RIM = 11;
NOTE_COWBELL = 12;

clap.sample_idx = 1;
ride.sample_idx = 2;
hat.sample_idx = 3;
shaker.sample_idx = 4;
rim.sample_idx = 5;

israte = 1.0 / srate;
freemem = midi.initializeMIDI(free_mem, 1, 1);
freemem = (audio_buffer = freemem) + 2 * osc_buffer;
freemem = (note_mapping = freemem) + 256;
(notes_initialized == 0) ? (
  memset(note_mapping, 0, 256);
  
  note_mapping[60] = NOTE_KICK;
  note_mapping[62] = NOTE_SNARE;
  note_mapping[64] = NOTE_CLAP;
  note_mapping[65] = NOTE_CLOSED_HAT;
  note_mapping[66] = NOTE_OPEN_HAT;
  note_mapping[67] = NOTE_RIDE;
  note_mapping[68] = NOTE_HI_TOM;
  note_mapping[69] = NOTE_MID_TOM;
  note_mapping[70] = NOTE_LOW_TOM;
  note_mapping[71] = NOTE_RIM;
  note_mapping[72] = NOTE_COWBELL;
  note_mapping[73] = NOTE_SHAKER;
  
  notes_initialized = 1;
);

freemem = (active_notes = freemem) + 256;

// Sample locations are recorded as [64 byte header (first is length), sampledata]
init_sample_data_defaults();
freemem = (samplelocs = freemem) + 128;
idx = 0;
loop(N_SAMPLES,
  freemem = (samplelocs[idx] = freemem) + SAMPLE_SIZE;
  idx += 1;
);

audio_buffer_idx = 0;

function clamp()
local()
instance(slider_idx, min_value, max_value)
(
  slider(slider_idx) > max_value ? (
    slider(slider_idx) = max_value;
  );
  slider(slider_idx) < min_value ? (
    slider(slider_idx) = min_value;
  );
);

function lerp_output_settings_block(me)
local(pan, new_panning, new_gain)
global(samplesblock, invblock, MIN_GAIN, MAX_GAIN, mute, solo, mmm)
instance(
  current_left, current_right, delta_left, delta_right, target_left, target_right, vel,
  gain_slider, pan_slider, gain_slider_vel, pan_slider_vel, total_gain,
  alive,
)
(
  new_panning = min(1, max(-1, slider(pan_slider) + vel * slider(pan_slider_vel)));
  new_gain = min(MAX_GAIN, max(MIN_GAIN, slider(gain_slider) + vel * slider(gain_slider_vel)));

  pan = 0.5 * (new_panning + 1);
  total_gain = pow(10, new_gain / 20);
  
  mute & (1 << me) ? ( total_gain = 0; );
  
  target_left = total_gain * sin(0.5 * (1.0 - pan) * $pi);
  target_right = total_gain * sin(0.5 * pan * $pi);

  delta_left = (target_left - current_left) * invblock;
  delta_right = (target_right - current_right) * invblock;

  ((abs(delta_left) + abs(delta_right)) > 0.00000001) ? (
    alive = samplesblock;
  );
);

function hard_update()
local()
global(samplesblock)
instance(
  current_left, current_right, delta_left, delta_right, target_left, target_right,
  vel,
  last_vel,
  alive,
)
(
  // Only force a vel update immediately if it's louder (otherwise things get clicky)
  (vel > last_vel) ? (
    this.lerp_output_settings_block();
    
    current_left = target_left;
    current_right = target_right;
    delta_left = 0;
    delta_right = 0;
    
    alive = samplesblock;
  );
  last_vel = vel;
);

function kick_update(_vel)
instance(vel)
(
  vel = _vel;
  kick_pitch_decay = max(0, min(1, _kick_pitch_decay + vel * kick_pitch_decay_vel));
  kick_tune = max(0, min(1, _kick_tune + vel * kick_tune_vel));
  kick_amp_decay = max(0, min(1, _kick_amp_decay + vel * kick_amp_decay_vel));
  kick_envelope = max(0, min(0.5, _kick_envelope + vel * kick_envelope_vel));
  kick_noise_envelope = max(0, min(1, _kick_noise_envelope + vel * kick_noise_envelope_vel));
  kick_decay = max(0, min(1, _kick_decay + vel * kick_decay_vel));
  
  this.hard_update();
);

