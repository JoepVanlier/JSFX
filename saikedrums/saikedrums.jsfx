function draw_pad(me, chan, cy, pad_size, knob_radius, knob_spacing, label, hint, override_ly, has_advanced)
local(ys, tapped, highlight, cy2, am_muted, am_solo, ret)
global(cx, select, pad_spacing, mute, solo, mouse_nx, mouse_ny, mouse_cap, mouse_x, mouse_y, gfx_x, gfx_y, last_cap, samplelocs, file_dropped)
instance(hly, is_closed, height, max_y, gain, total_gain, sample_idx)
(
  gfx_set(1, 1, 1, 1);
  this.txt_measure(label);
  
  gfx_set(0.21, 0.21, 0.62, 1.0);
  height = 6 * pad_size;
  gfx_rect(cx - 2, cy + 6, pad_size + 4, height, 0);
  max_y = cy + 6 + height;
  
  has_advanced ? (
    this.draw_advanced(me, cx + pad_size - 1, cy + height + 3);
  );
  
  ys = cy;
  gfx_set(0.02, 0.02, 0.02, 1);
  
  // TO DO: Finish solo implementation
  am_muted = mute & (1 << chan);
  am_muted ? gfx_set(0.02, 0.02, 0.02, 0.3);
  
  /*am_solo = solo & (1 << me);
  am_solo ? gfx_set(0.25, 1.0, 0.0, 0.5);*/
  txt_blit(label, cx + 0.5 * (pad_size - this.txt_w + 1), cy);
  
  ((mouse_nx > cx - 2) && (mouse_nx < cx + pad_size - 1) && (mouse_ny > cy) && (mouse_ny < (cy + 5) && (last_cap == 0)) ? (
    (mouse_cap == 1)) ? (
      am_muted ? (
        mute -= (1 << chan);
      ) : (
        mute += (1 << chan);
      );
    ) : (
    (mouse_cap == 2)) ? (
      gfx_x = mouse_x;
      gfx_y = mouse_y;
      ret = gfx_showmenu("Unmute All|Mute All");
      
      (ret == 2) ? (
        mute = 8191
      ) : (ret == 1) ? (
        mute = 0;
      );
    
      /*am_solo ? (
        solo -= (1 << chan);
      ) : (
        solo += (1 << chan);
      );*/
    );
  );
  
  cy += 8;

  gfx_set(1, 1, 1, 1);
  
  cy2 = ceil(cy + pad_size + pad_size / 2) + 4;
  this.draw_types(me, chan, cx, cy2 - 9);
  this.gain.draw_knob(cx + floor(pad_size / 2), cy2 + knob_spacing, knob_radius, me / 11, "Gain");
  this.pan.draw_knob(cx + floor(pad_size / 2), cy2 + 2 * knob_spacing + ceil(2 * knob_radius), knob_radius, me / 11, "Pan");
  (me == 2) ? (  // :
    this.tune.draw_knob(cx + floor(pad_size / 2), cy2 + 3 * knob_spacing + ceil(4 * knob_radius), knob_radius, me / 11, "Decay");
    this.decay.draw_knob(cx + floor(pad_size / 2), cy2 + 4 * knob_spacing + ceil(6 * knob_radius), knob_radius, me / 11, "Space");
  ) : (
    this.tune.draw_knob(cx + floor(pad_size / 2), cy2 + 3 * knob_spacing + ceil(4 * knob_radius), knob_radius, me / 11, "Tune");
    this.decay.draw_knob(cx + floor(pad_size / 2), cy2 + 4 * knob_spacing + ceil(6 * knob_radius), knob_radius, me / 11, "Decay");
  );
  
  colormodel(me / 11);
  tapped = 0;
  
  /* Annoying specialization for the hihat which uses two pads for one audio stream */
  highlight = 0;
  override_ly ? (
    (override_ly == 1) ? (
      is_closed ? (
        highlight = hly * total_gain;
        hly = 0;
      );
    ) : (
      (is_closed == 0) ? (
        highlight = hly * total_gain;
        hly = 0;
      );
    );
  ) : (
    highlight = hly * total_gain;
    hly = 0;
  );
  
  this.tap_pad(me, cx, cy, pad_size, select, hint, 2.0 * highlight, (sample_idx > 0) && (mouse_cap == 0) && (gfx_getchar(65536) & 2 == 0)) ? (
    tapped = 1;
  );
  
  sample_idx ? (
    this.process_sample_drop(sample_idx, samplelocs[sample_idx - 1], this.over);
  );
  
  am_muted ? (
    gfx_set(0, 0, 0, 0.3);
    gfx_rect(cx - 1, ys, pad_size + 2, 107);
  );
  
  cx += pad_size + pad_spacing;
  
  tapped & !am_muted
);


