function draw_advanced(me, tx, ty, ww, thcolor, label, info)
local(intensity)
global(advanced_open, mouse_nx, mouse_ny, mouse_cap, last_cap, gfx_r, gfx_g, gfx_b, gfx_mode, gfx_a, info_active)
instance(over_advanced, advanced, advanced_highlight)
(
  over_advanced = (mouse_nx >= tx) && (mouse_nx <= tx + ww) && (mouse_ny >= ty) && (mouse_ny <= ty + 6);
  advanced_highlight = 0.5 * advanced_highlight + 0.5 * (over_advanced && (mouse_cap == 0));
  
  over_advanced ? (
    sprintf(14, "%s", info);
    info_active = 1;
  );
  
  (advanced_open == me) ? (
    intensity = 0.8;
  ) : (
    intensity = 0.3 + 0.15 * advanced_highlight * abs(sin(10 * time_precise()));
  );
  colormodel(thcolor);
  gfx_r = intensity * gfx_r;
  gfx_g = intensity * gfx_g;
  gfx_b = intensity * gfx_b;
  gfx_rect(tx, ty, ww, 6, 1);
  gfx_mode = 1;
  gfx_a = 0.6;
  txt_blit(label, tx + 1, ty + 1);
  
  (over_advanced && (mouse_cap & 1 == 1) && (last_cap == 0)) ? (
    (advanced_open != me) ? (
      advanced_open = me;
    ) : (
      advanced_open = 0;
    );
  );
);

