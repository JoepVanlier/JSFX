      x *= .05;
      
      // First guess based on linear system
      y1 = (d0*h - d1*hh - d1*k2h + d1 - d2*h2 + h*x) * denom;
      y2 = (d0*hh + d1*h2 - d2*hh + d2*k2h + d2 + hh*x) * denom;
      
