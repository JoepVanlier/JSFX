instance(coeff, b1, b2, b3, K, a0)
local(i1g, gg, f0, g, ga)
global(isampling_factor)
(
  K = 2 * resonance;
  f0 = exp( (1 - freq) * log(20/22050) ) * isampling_factor;
  g = tan(.5 * $pi * f0);
  i1g = 1.0 / (1.0 + g);
  coeff = g * i1g;
  ga = 2.0 * coeff - 1.0;
  b1 = ga * coeff;
  b2 = ga * i1g;
  b3 = 2.0 * i1g;
  a0 = 1.0 / (1.0 + K * ga * coeff * coeff);
