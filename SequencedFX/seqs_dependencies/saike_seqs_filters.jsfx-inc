    //       x   y1   y2
    // LP    0    0    1
    // BP    0    1    0
    // HP    1  2*k   -1
    // BR    1   -1    0
    ( morph < 0.25 ) ? (
      mo0 = 0;
      mo1 = 16.0*morph;
      mo2 = 4 - 16.0*morph;
    ) : ( morph < 0.5 ) ? (
      mo0 = 16.0*morph - 4.0;
