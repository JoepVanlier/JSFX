  instance(x, y, w, h, r, g, b, a, default, minval, maxval, value, value2, label, lastleft, lastright, lastclick, yslidercenter, cap, twoval, thisUI, htime, hint, onmarker)
  (
    left  = mc & 1;
    right = mc & 2;
    shft  = mc & 8;
    
    change = 0;
