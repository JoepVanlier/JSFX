function init_wasplin(freq, res)
  global(srate, slider54)
  local(f, h, hh, itau, wrel, rate)
  instance(U0, U1, Di, D0, D1, x0, x1, x2, x3, i1, K2)
  (
    rate    = (srate * max(slider54,1));
    wrel    = 2 * $pi * 22050 * ( ( pow(10, freq) - 1 ) / 9 );
    f       = 20 * rate * tan( .5 * wrel * h );
    h       = 1.0 / rate;
    hh      = 0.5 * h;
    itau    = .08 * f;
    K2      = .8-.6*res;
    
    x0 = hh*itau;
    x1 = 2*x0;
    x2 = K2*x1;
    x3 = 4*x0*x0;
    i1 = 1.0/(x2 + x3 + 1);
  );
  
