function gen_dynamics(xp, yp, wp, hp)
  instance(x, y, w, h, yref, sc, cap_mode, inLight, outLight, dynLight, lastleft)
  global(inLight.toggleLight)
  local()
  (
    x = xp;
    y = yp;
    w = wp;
    h = hp;
