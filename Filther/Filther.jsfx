function tanh(s)
local(c)
global()
instance()
(
  c = exp(8*s);
  s = 0.25*(c - 1) / (c + 1);
);

function fasttanh(x)
local()
global()
instance()
(
  x = x / (1 + abs(4*x));
  /*( x < -3 ) ? (
      x = -1;
  ) : ( x > 3 ) ? (
