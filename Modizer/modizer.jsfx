followPitch.drawToggle(cX - 0.95 * knobSize, cY - 0.95 * knobSize, 0.18 * knobSize, 0.18 * knobSize, dont_follow_pitch, widget_r, widget_g, widget_b, widget_a, toggle_r, toggle_g, toggle_b, toggle_a, "Enabling this button will ignore incoming MIDI.");
dont_follow_pitch = followPitch.processMouseToggle(mouse_x, mouse_y, mouse_cap);

cX += 2.5*stepSize;

bitKnob.knob_set(sprintf(1, "%d", bitrate + .001), 1, (bitrate - 2) / (24 - 2));
bitKnob.drawKnob(cX, cY, knobSize, "Bitrate", "Pseudo bitrate.\n\nNote that ", 0);
