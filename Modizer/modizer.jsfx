function blit_letter(destx, desty, char, warp)
(
  scale = 1.5;
  gfx_deltablit(5, scale, char*58 - 1, cw, ch, destx, desty, cw*scale, ch*scale, 2/scale, 0, 0, 2/scale, warp, warp, 0);
);

current_char += 0.25;
offset = current_char - floor(current_char + .00001);

gfx_mode = 1;
cs = current_char;
xx = 0;
gfx_a = 1;
y_left = gfx_h - 2 * knobSize;
scroller_middle = gfx_h - 0.5 * y_left;
loop(gfx_w / 30 + 3,
  cs > str_len ? cs -= str_len;
  blit_letter(-50 + 30 * (xx - offset), scroller_middle + 15*sin(5*time_precise() + .1*xx), str[cs] - 33, -0.004 * sin(5*time_precise() + .1*xx));
  cs += 1;
  xx += 1;
);

knobSize = 40;
stepSize = knobSize * (gfx_w/400);
cX = 0.5 * (gfx_w - 9 * stepSize);
cY = 50;

lpToggle.label = sprintf(33, " LED Filter");
lpToggle.drawToggle(cX, cy - .6 * knobSize, .15*knobSize, .15*knobSize, paula, widget_r, widget_g, widget_b, widget_a, toggle_r, toggle_g, toggle_b, toggle_a, "Enabling this enables the LED\nfilter on the Amiga (lowpass\nfilter on the output)");
paula = lpToggle.processMouseToggle(mouse_x, mouse_y, mouse_cap);


