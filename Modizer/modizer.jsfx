function init_sallen(cutoff, res)
instance(a0, a1, a2, a3, a4, a5, ic1eq, ic2eq, k)
local(g, pg1)
global(srate)
(
  g = tan($pi * cutoff/srate);
  k = 2.0 * res;
  pg1 = 1.0 + g;
  a0 = 1.0/(pg1 * pg1 - g * k);
  a1 = k * a0;
  a2 = pg1 * a0;
  a3 = g * a2;
  a4 = 1.0 / pg1;
  a5 = g * a4;
  ic1eq = 0.0;
  ic2eq = 0.0;
);

function eval_sallen(x)
instance(a0, a1, a2, a3, a4, a5, ic1eq, ic2eq, k)
local(v1, v2)
global()
(
  v1 = a1 * ic2eq + a2 * ic1eq + a3 * x;
  v2 = a4 * ic2eq + a5 * v1;
  ic1eq = 2.0 * (v1 - k * v2) - ic1eq;
  ic2eq = 2.0 * v2 - ic2eq;
  
  v2
);

function eval_rc(x)
instance(s, coeff)
global()
local(y, v)
(
  v = coeff*(x - s);
  y = v + s;
  s = (y + v + coeff*x) / (1+coeff);
  y
);
  
function init_rc(cutoff)
instance(coeff)
global(srate)
local(g)
( 
  g = tan($pi*cutoff/srate);
  coeff = g/(1+g);  
);

sallen_left.init_sallen(3275, sqrt(0.125));
sallen_right.init_sallen(3275, sqrt(0.125));
rc_left.init_rc(4421);
rc_right.init_rc(4421);
rc_hf_left.init_rc(16);
rc_hf_right.init_rc(16);

function update_bitrate()
(
  b_factor = 2^(bitrate - 1);
  ib_factor = 1/b_factor;
);

function update_srate()
(
  ((current_note_played == 0) || dont_follow_pitch) ? current_note_played = 69;
  
  noteFactor = 2^((record_pitch - current_note_played)/12);
  factor = noteFactor * (srate / samplerate);
);

update_bitrate();
update_srate();

@slider
cpos = 0;
update_bitrate_now = 1;

@block
midi.processMIDIBlock();
update_bitrate();
update_srate();

@sample
update_bitrate_now ? (
  update_bitrate();
  update_srate();
  update_bitrate_now = 0;
);

midi.processMIDISample() ? (
  (midi.lastNote > 0) ? current_note_played = midi.lastNote;
  update_srate();
  nodata = 0;
) : ( nodata += .1 );

l = ib_factor * floor(spl0 * b_factor);
r = ib_factor * floor(spl1 * b_factor);

cpos += 1;

function interp(x0, frac)
instance(x1, x2, x3)
local(id0, id1, id2, id3)
global()
(
  id0 = x1;
  id1 = 0.5*(x2 - x0);
  id2 = x0 - 2.5*x1 + 2*x2 - 0.5*x3;
  id3 = 0.5*(x3 - x0) + 1.5 * (x1 - x2);
  
  x3 = x2;
  x2 = x1;
  x1 = x0;
  
  ((id3*frac+id2)*frac+id1)*frac+id0
);

function hist(x0)
instance(x1, x2, x3)
local()
global()
(
  x3 = x2;
  x2 = x1;
  x1 = x0;
);

// Only mess with the sampling if the factor is actually > 1.
c += 1;
factor > 1 ? (
  // "S&H"
  cpos > factor ? (
    while(
      cpos -= factor;
      cpos > factor
    );
    
    from_l = to_l;
    from_r = to_r;
    to_l = l_hist.interp(l, cpos - floor(cpos));
    to_r = r_hist.interp(r, cpos - floor(cpos));
  );
  
  l_hist.hist(l);
  r_hist.hist(r);

  // "Linear"
  interp ? (
    progress = cpos/factor;
    l = to_l * progress + from_l * (1-progress);
    r = to_r * progress + from_r * (1-progress);
  ) : (
    l = to_l;
    r = to_r;
  );
);

// Amiga lowpasses
l = rc_left.eval_rc(l);
r = rc_right.eval_rc(r);

l = l - rc_hf_left.eval_rc(l);
r = r - rc_hf_right.eval_rc(r);

paula ? (
  l = sallen_left.eval_sallen(l);
  r = sallen_right.eval_sallen(r);
);

spl0 = l;
spl1 = r;
  
