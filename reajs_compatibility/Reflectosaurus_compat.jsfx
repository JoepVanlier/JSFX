lsMod = lsMod * (1-recSpeed) + lightState * recSpeed;
gfx_a = .2 + .8*lsMod * (.96+.04*sin(314*render_time));
rand() > lightProb ? (
  lightState = 1-lightState;
);
lightState ? (
