        r = shaped_r;
      );
    
      l = activevol*l*lPanTerm*play;
      r = activevol*r*rPanTerm*play;
      
      lOut += l;
      rOut += r;
    );
    
    level = .5*(abs(l)+abs(r))*play;
  );

