  (
    y > 1 ? y = 1;
    !freeNodes ? ( x > 1 ? x = 1; );
    y < 0 ? y = 0;
    x < 0 ? x = 0;
    
    ( !mute || solo ) ? (
      ctime = dtime;
      pitchTracking && pitchmod ? ctime *= pitchMod;
      newdtime = update_alpha * (srate * ctime + timeOffset) + inv_update_alpha * olddtime;
      
