      /* Crossover distortion */
      l = l * ceiling;
      r = r * ceiling;
      ( l > 1 ) ? ( l = 1 ) : ( l < -1 ) ? ( l = -1 );
      ( r > 1 ) ? ( r = 1 ) : ( r < -1 ) ? ( r = -1 );
          
      factor = -15.09587456;
          
      t = l*l;
      l = (0.78779516*l/(1+abs(l)-3.39263197*t+2.20506682*abs(l)*t)) * (1-exp(factor*t));
      t = r*r;
      r = (0.78779516*r/(1+abs(r)-3.39263197*t+2.20506682*abs(r)*t)) * (1-exp(factor*t));
