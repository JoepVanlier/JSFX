    maxTime == 0 ? maxTime = 4;
    
    /* Initialize or reset the nodes */
    node1.initNode(dTap1,   xToTime(0.5),  0.1, 1,  0, pitchBufferLocation1, fftmem1, fftmem2);
    node2.initNode(dTap2,   xToTime(0.1),  1.0, 2,  0, pitchBufferLocation2, fftmem1, fftmem2);
