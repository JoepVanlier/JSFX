        lfoTime += lfoAdvanceStep;
        dTimeOffset = sin(lfoTime) * lfoDepthFactor;
        diff = 1;
      ) : diff = olddtime - newdtime;
    
      isReverb ? (
        xSmoothed == 0 ? xSmoothed = x;
