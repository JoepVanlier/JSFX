function node(x, y, w, h)
instance(_x, _y, _w, _h, x_from, x_to, y_from, y_to)
(
  _x = x; _y = y; _w = w; _h = h;
  gfx_rect(x, y, w, h);
  x_from = x + w;
  y_from = y + 0.5 * h;
  x_to = x;
  y_to = y + 0.5 * h;
);

